# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
indent_size = 2
indent_style = space
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true
charset = utf-8
quote_type = single

[*.css]
indent_size = 4

[*Makefile]
indent_style = tab
indent_size = 4
